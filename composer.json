{
  "name": "edgar/ez-webpush-bundle",
  "license": "GPL-2.0-only",
  "type": "ezplatform-bundle",
  "description": "eZ Platform web push notification",
  "authors": [
    {
      "name": "Emmanuel DROUET"
    }
  ],
  "autoload": {
    "psr-4": {
      "Edgar\\EzWebPushBundle\\": "src/bundle/",
      "Edgar\\EzWebPush\\": "src/lib/"
    }
  },
  "autoload-dev": {
    "psr-4": { "Edgar\\EzWebPushBundle\\Tests\\": "src/bundle/Tests" }
  },
  "require": {
    "php": ">=7.1",
    "ezsystems/ezpublish-kernel": "^7.2",
    "ezsystems/ezplatform-admin-ui": "^1.2",
    "edgar/ez-uiprofile-bundle": "~3.0",
    "minishlink/web-push": "~4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "~2.7.1",
    "phpunit/phpunit": "^6.4"
  },
  "scripts": {
    "fix-cs": "@php ./vendor/bin/php-cs-fixer fix -v --show-progress=estimating"
  }
}
